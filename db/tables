CREATE TABLE UsersDetail(
email varchar(50) NOT NULL,
Name varchar(50) NOT NULL,
image varchar(255) NOT NULL,
PHONE varchar(20) NOT NULL,
PRIMARY KEY (email)
);
create table users(
 Login_id varchar(50) Not Null,
  Password varchar (100) Not Null,
 PRIMARY KEY ( Login_id)
);
RegisterDELIMITER //
 
CREATE PROCEDURE Register( IN email varchar(50),IN Password varchar(100),IN  Name varchar(50),IN Phone varchar(20),IN image varchar(255))

BEGIN

   
    
    INSERT INTO  users  values (email,Password);
    INSERT INTO   usersdetail values (email,Name,PHONE,image);
	


        END //

DELIMITER ;
